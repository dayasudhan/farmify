
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Products {
  id        Int      @default(autoincrement()) @id
  name     String?
  year     String?
  image_urls String[]
  price   Int?
  availability Int?
  description   String?
  // price    String?
  // content   String?
  // published Boolean? @default(false)
  // author    User?    @relation(fields: [authorId], references: [id])
  // authorId  Int?
   @@map("products")
}
model People {
  id    Int     @default(autoincrement()) @id
  //email String  @unique
  firstName  String?
  lastName  String?
  @@map("people")
}

